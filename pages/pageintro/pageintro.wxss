page {
  background-color: #f2f2f2;
}


.intro-out {
  width: 600rpx;
  height: 830rpx;
  margin: 11rpx auto 0;
  background-color: #fff;
  margin-bottom: 24rpx;
  position: relative;
}

.intro-show {
  position: absolute;
  width: 100%;
  height: 100%;
}

.intro-show.unselected{
  opacity: 0;
}



.btn-skip{
  position: absolute;
  width: 101rpx;
  height: 45rpx;
  bottom: 25rpx;
  right:30rpx;
}

.intro-menu {
  width: 574rpx;
  height: 78rpx;
  line-height: 78rpx;
  overflow: hidden;
  font-size: 26rpx;
  color: #6b6b6b;
  padding-left: 35rpx;
  margin: 0 auto;
  position: relative;
}

.intro-menu .intro-icon {
  width: 10rpx;
  height: 78rpx;
  position: absolute;
  left: 14rpx;
}

.intro-menu.selected {
  background-repeat:no-repeat; 
  background-size:100% 100%;
  background-image:url(data:image/png;base64,);
}